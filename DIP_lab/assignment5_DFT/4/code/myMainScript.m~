%% MyMainScript

tic;
%% Your code here
im=imread('../data/barbara256.png');


[h,w]=size(im);
imnew=padarray(im,[h/2,w/2]);
imf=fft2(imnew);
im1=fftshift(imf);


D=[40 80];

 [output,H]=lowpassfilter(im1,h,w,D(1));
 figure(1)
  title('A');
 imagesc(log(abs(H))+1);
 colorbar;
 figure(2)
title('lowpassFilteredImage D=40');
 imshow(mat2gray(output));


 [output1,H1]=lowpassfilter(im1,h,w,D(2));
 figure(3)
 title('frequencyLowfilter D=80');
 imagesc(log(abs(H1))+1);
 
 colorbar;
 figure(4)
title('lowpassFilteredImage D=80');
 imshow(mat2gray(output1));







sigma=[40 80];
% gaussianfilter(im1,h,w,sigma(1));
% gaussianfilter(im1,h,w,sigma(2));



toc;


